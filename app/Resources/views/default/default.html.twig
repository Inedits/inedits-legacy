{% extends "FOSUserBundle::layout.html.twig" %}
{% block title %}Accueil{% endblock %}
{% block body %}
    <div class="container">
        <section>
            <h1>Derniers arbres</h1>
            <div class="row">
                <div class="col-xs-12 col-xs-4">
                    {% for tree in trees %}
                        {{ include('post/_tree_box.html.twig', { tree: tree.Tree, users: tree.Users }, with_context = false) }}
                    {% endfor %}
                </div>
            </div>
        </section>
    </div>
{% endblock %}
